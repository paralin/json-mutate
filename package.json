{
  "name": "json-mutate",
  "version": "1.1.1",
  "description": "Generate and apply descriptions of changes between objects.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "lint": "npm run lint:js",
    "lint:js": "eslint -c .eslintrc.js --ext .ts ./js/**/*.ts --fix",
    "build": "tsc",
    "test": "npm run mocha",
    "ci": "npm run build && npm run test && npm run lint",
    "mocha": "ts-node node_modules/istanbul/lib/cli.js cover -e .ts  -x \"js/*.d.ts\" -x \"js/*.spec.ts\" node_modules/jasmine/bin/jasmine.js -- JASMINE_CONFIG_PATH=jasmine.json",
    "prepublish": "npm run build && npm run test",
    "postpublish": "rm README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paralin/json-mutate.git"
  },
  "keywords": [
    "mutate",
    "js",
    "typescript"
  ],
  "author": "Christian Stewart <christian@paral.in>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paralin/json-mutate/issues"
  },
  "homepage": "https://github.com/paralin/json-mutate#readme",
  "devDependencies": {
    "@types/jasmine": "^5.1.4",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "babel-core": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "istanbul": "^1.1.0-alpha.1",
    "jasmine": "^5.1.0",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "eslint-plugin-unused-imports": "^3.0.0"
  }
}
